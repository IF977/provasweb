<h1>Listing quests</h1>

<table>
  <thead>
    <tr>
      <th>Level</th>
      <th>Discipline</th>
      <th>Subject</th>
      <th>Question</th>
      <th>Teacher</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @quests.each do |quest| %>
      <tr>
        <td><%= quest.level %></td>
        <td><%= quest.discipline %></td>
        <td><%= quest.subject %></td>
        <td><%= quest.question %></td>
        <td><%= quest.teacher %></td>
        <td><%= link_to 'Show', quest %></td>
        <td><%= link_to 'Edit', edit_quest_path(quest) %></td>
        <td><%= link_to 'Destroy', quest, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Quest', new_quest_path %>
