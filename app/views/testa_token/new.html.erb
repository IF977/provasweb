<h1>Acessar avaliação</h1>
<%= form_tag testa_token_index_path do %>

  <div class="control-group">
    <%= label_tag :name%>
    <div class="controls">
      <%= text_field_tag :name, params[:name], :class => 'form-control' %>
    </div>
   
  </div>
    <div class="control-group">
    <%= label_tag :email%>
    <div class="controls">
      <%= text_field_tag :email, params[:email], :class => 'form-control' %>
    </div>
   
  </div>
  <div class="control-group">
    <%= label_tag "Token" %>
    <div class="controls">
      <%= text_field_tag :token, params[:token], :class => 'form-control' %>
    </div>

  </div>

  <%= submit_tag "Logar" , :class => 'btn btn-primary' %>

<% end %>
