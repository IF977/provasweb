<h1>Listing profs</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Email</th>
      <th>Senha</th>
      <th>Login</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @profs.each do |prof| %>
      <tr>
        <td><%= prof.nome %></td>
        <td><%= prof.email %></td>
        <td><%= prof.senha %></td>
        <td><%= prof.login %></td>
        <td><%= link_to 'Show', prof %></td>
        <td><%= link_to 'Edit', edit_prof_path(prof) %></td>
        <td><%= link_to 'Destroy', prof, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Prof', new_prof_path %>
