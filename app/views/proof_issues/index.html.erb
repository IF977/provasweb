<h1>Listing proof_issues</h1>

<table>
  <thead>
    <tr>
      <th>Proof</th>
      <th>Issue</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% #@proof_issues.each do |proof_issue| %>
        <% @questoesProva.each do |proof_issue| %>
      <tr>
        <td><%= proof_issue.proof.id %></td>
        <td><%= proof_issue.issue.inunciation %></td>
          <% proof_issue.issue.alternatives.each do |alternativas| %>
            <td><%= alternativas.description %></td>
           <% end %>
        <td><%= link_to 'Show', proof_issue %></td>
        <td><%= link_to 'Edit', edit_proof_issue_path(proof_issue) %></td>
        <td><%= link_to 'Destroy', proof_issue, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Proof issue', new_proof_issue_path %>
